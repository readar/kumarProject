<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;com.mytraining.facades.product.data.GenderData>"/>
	</bean>

	<bean class="com.mytraining.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>
	
	<bean class="com.mytraining.facades.product.data.ManojProductData">
		<property name="productId" type="String"/>
		<property name="productName" type="String"/>
		<property name="productInfo" type="String"/>
		<property name="productPrice" type="java.lang.Double"/>
		<property name="manojCategoryData" type="com.mytraining.facades.product.data.ManojCategoryData"></property>
	</bean>
	
	<bean class="com.mytraining.facades.product.data.ManojCategoryData">
		<property name="categoryCode" type="String"/>
		<property name="categoryName" type="String"/>
		<property name="categoryInfo" type="String"/>
	</bean>
</beans>
